#include "stdafx.h"
#include "iostream"
using namespace std;

int main()
{
	float a1,a2,b1,b2,c1,c2,x,x1,x2;
	cin >> a1;
	cin >> b1;
	cin >> c1;	
	cin >> a2;
	cin >> b2;
	cin >> c2;
	x = a1 * b2 - a2 * b1;
	x1 = c1 * b2 - c2 * b1;
	x2 = a1 * c2 - a2 * c1;
	x1 = x1 / x;
	x2 = x2 / x;
	cout << "x1 = "<< x1 << "\t x2 = " << x2 << endl;
}
